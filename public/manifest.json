{
    "manifest_version": 3,
    "name": "OneTab",
    "version": "1.0",
    "description": "OneTab is the best way to convert all of your tabs into a list and save memory on your computer.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "48": "icon.png"
    }
}